{
  "name": "Decibel",
  "version": "0.0.1",
  "description": "Voice Chat app",
  "main": "main.js",
  "scripts": {
    "build-js": "webpack",
    "watch-js": "webpack --watch",
    "build-css": "node-sass --include-path scss src/style.scss bin/app-style.css --output-style compressed",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-all": "concurrently \"npm run build-css\" \"npm run build-js\"",
    "watch-all": "concurrently \"npm run watch-css\" \"npm run watch-js\"",
    "start": "electron ."
  },
  "author": "Dominic Maas",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "concurrently": "^3.4.0",
    "electron-reload": "^1.1.0",
    "node-sass": "^4.5.1",
    "nodemon": "^1.11.0",
    "uglifyjs-webpack-plugin": "^0.3.1",
    "webpack": "^2.3.2"
  },
  "dependencies": {
    "background-blur": "^0.1.3",
    "electron": "^1.6.4",
    "electron-config": "^0.2.1",
    "jquery": "^3.2.1",
    "material-ui": "^0.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tap-event-plugin": "^2.0.1",
    "ws": "^2.2.2"
  }
}
