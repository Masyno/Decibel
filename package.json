{
  "name": "Decibel",
  "version": "0.0.1",
  "description": "Voice Chat app",
  "main": "main.js",
  "scripts": {
    "build-js": "webpack",
    "watch-js": "webpack --watch",
    "build-css": "node-sass --include-path scss src/style.scss bin/app-style.css --output-style compressed",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-all": "concurrently \"npm run build-css\" \"npm run build-js\"",
    "start": "electron ."
  },
  "author": "Dominic Maas",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "concurrently": "^3.4.0",
    "node-sass": "^4.5.1",
    "nodemon": "^1.11.0",
    "uglifyjs-webpack-plugin": "^0.3.1",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "background-blur": "^0.1.3",
    "electron": "^1.6.4",
    "jquery": "^3.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "ws": "^2.2.1"
  }
}
